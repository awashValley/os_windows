
## change working directory
   > cd/d c:\Users\mrx\Desktop
   
## list docs
   >dir
   
## display global environmental variables
   > echo %PATH%
   
## paste
   > right click

## list files only, i.e., without date, time, etc. ; 
   >dir /b 
   >dir /b *.log

## display subfolders. ;
   >dir /b /s
   >dir /b /s *.log
   
## search text in a file. ;
   >find "pay" myfile.txt
   
## copy a file.;
   >copy con search.txt
   
## find list of strings stored in search.txt from log file
## the option 'i' is used for case insensitive feature
   >findstr /i /n /g:search.txt mylog.log

## find list of strings stored in search.txt from log file, except the other values stored in search_not.txt
   >findstr /i /n /g:search.txt mylog.log | findstr /i /iv /g:search_not.txt
